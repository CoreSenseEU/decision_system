<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <!-- Take Meta -->
  <BehaviorTree ID="MakeSureChoiceTaken_Template">
    <Sequence _successIf="choice_last_updated_iteration >= iteration">
      <SubTree ID="MakeSureAlternativesOrdered_Template" _autoremap="true" />
      <Take ordering="{ordering}"
            choice="{choice}"
            action_name=""/>
      <Script code="choice_last_updated_iteration = iteration" />
    </Sequence>
  </BehaviorTree>
</root>
