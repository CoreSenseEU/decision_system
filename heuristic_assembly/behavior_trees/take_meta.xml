<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <!-- Take Meta -->
  <BehaviorTree ID="MakeSureChoiceTaken">
    <!-- Assume a choice will never be up to date if this is ticked, so no need for a fallback -->
    <Sequence>
      <SubTree ID="MakeSureAlternativesOrdered" _autoremap="true" />
      <Take ordering="{ordering}"
            choice="{choice}"
            action_name=""/>
    </Sequence>
  </BehaviorTree>
</root>
