<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="TestOrderLexicographical">
    <Sequence>
      <Assess alternatives="blue;red;#808006"
              cues="r;g"
              assessments="{assessments}"
              action_name="Assess"/>
      <Aggregate assessments="{assessments}"
                 evaluation="{evaluation}"
                 action_name="Aggregate"/>
      <Order evaluation="{evaluation}"
             ordering="{ordering}"
             action_name="order_lexicographical_action_server/Order"/>
    </Sequence>
  </BehaviorTree>
</root>
